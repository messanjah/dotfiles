[color]
    ui = auto
  [color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
  [color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
  [color "status"]
    added = yellow
    changed = green
    untracked = cyan

  [color "diff"]
    whitespace = red reverse
  [core]
    whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol
    autocrlf = false

  [alias]
    aa = add --all --intent-to-add
    ap = add --patch
    st = status
    ci = commit -am
    br = branch
    co = checkout
    df = diff
    dfm = diff origin/master
    dc = diff --cached
    lg = log -p
    lol = log --graph --decorate --pretty=oneline --abbrev-commit
    lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
    ls = ls-files
    pm = pull origin master
    fo = fetch origin
    ps = push origin HEAD
[user]
  name = Tyler Naumu
  email = tyler@procore.com
[push]
	default = simple
